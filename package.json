{
  "name": "salon_appointment",
  "version": "1.0.0",
  "description": "Appointment Booking System for Sammy's Salon",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false && react-scripts build",
    "test": "node --max_old_space_size=8192 --expose-gc node_modules/.bin/react-scripts test --transformIgnorePatterns \"node_modules/(?!(axios|@emailjs/browser))/\" --testTimeout=30000",
    "test:coverage": "node --max_old_space_size=8192 --expose-gc node_modules/.bin/react-scripts test --coverage --watchAll=false --maxWorkers=2",
    "test:debug": "node --max_old_space_size=8192 --inspect-brk node_modules/.bin/react-scripts test --runInBand --no-cache",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rebussharma/salon_appointment.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rebussharma/salon_appointment/issues"
  },
  "homepage": "https://github.com/rebussharma/salon_appointment#readme",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "react": "^19.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.0.0",
    "react-scripts": "^5.0.1",
    "typescript": "^3.9.10",
    "ua-parser-js": "^2.0.0",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
